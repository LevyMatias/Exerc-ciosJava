public class Main {

    public static void main(String[] args) {
        
        Scanner input = new Scanner (System.in);
        
        double area, raio;
        
        raio = input.nextDouble();   
        
        area = 3.14159 * raio * raio;
        
        System.out.printf("A=%.4f\n",area);     
    }  
}
